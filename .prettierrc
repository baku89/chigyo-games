{
	"arrowParens": "avoid",
	"bracketSpacing": false,
	"endOfLine": "lf",
	"printWidth": 80,
	"proseWrap": "preserve",
	"semi": false,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"plugins": ["prettier-plugin-organize-imports"],
	"overrides": [
		{
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		}
	]
}
